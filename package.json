{
	"name": "my-clr-fun",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(html|vue|ts|js|json|cjs)\"",
		"type-check": "yarn vue-tsc --noEmit",
		"build-map": "NODE_OPTIONS=--max-old-space-size=4096 vite build --sourcemap",
		"test:lint-i18n": "vue-i18n-extract report --vueFiles 'src/**/*.?(vue)' --languageFiles 'src/locales/*.?(json)' --exclude dynamic"
	},
	"lint-staged": {
		"**/*.{js,ts,json,scss,css,vue}": [
			"yarn format"
		]
	},
	"dependencies": {
		"@coinbase/wallet-sdk": "^3.6.3",
		"@kleros/gtcr-encoder": "1.4.0",
		"@openfonts/inter_all": "^1.0.2",
		"@vuelidate/core": "^2.0.0",
		"@vuelidate/validators": "^2.0.0",
		"@vueuse/core": "^10.2.1",
		"@walletconnect/web3-provider": "^1.8.0",
		"clrfund-contracts": "^4.2.3-beta.0",
		"clrfund-maci-utils": "^0.0.1",
		"crypto-js": "^4.1.1",
		"ethereum-blockies-base64": "^1.0.2",
		"ethers": "^5.7.2",
		"floating-vue": "^2.0.0-y.0",
		"humanize-duration": "^3.27.3",
		"is-ipfs": "^7.0.3",
		"luxon": "^3.3.0",
		"markdown-it": "^13.0.1",
		"markdown-it-link-attributes": "^4.0.1",
		"node-stdlib-browser": "^1.2.0",
		"pinia": "^2.1.6",
		"pinia-plugin-persistedstate": "^3.2.0",
		"qrcode": "^1.5.1",
		"tiny-invariant": "^1.3.1",
		"viem": "^1.5.2",
		"vue": "^3.3.4",
		"vue-dapp": "^0.7.1",
		"vue-final-modal": "^4.4.5",
		"vue-i18n": "9",
		"vue-meta": "3.0.0-alpha.7",
		"vue-router": "^4.2.4",
		"vue3-select": "^0.1.0"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^0.8.1",
		"@rollup/plugin-inject": "5.0.2",
		"@rushstack/eslint-patch": "^1.2.0",
		"@types/humanize-duration": "^3.27.1",
		"@types/luxon": "^3.1.0",
		"@types/qrcode": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.44.0",
		"@typescript-eslint/parser": "^5.44.0",
		"@vitejs/plugin-vue": "3.2.0",
		"@vue/eslint-config-prettier": "^7.0.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.2.6",
		"@vue/tsconfig": "^0.1.3",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^9.8.0",
		"jsdom": "^20.0.3",
		"postcss": "^8.4.27",
		"prettier": "^2.8.0",
		"sass": "^1.64.1",
		"tailwindcss": "^3.3.3",
		"typescript": "^4.9.3",
		"unplugin-auto-import": "^0.11.5",
		"unplugin-vue-components": "^0.22.11",
		"vite": "3.2.4",
		"vitest": "^0.26.2",
		"vue-i18n-extract": "^2.0.7",
		"vue-tsc": "^1.8.8"
	}
}
